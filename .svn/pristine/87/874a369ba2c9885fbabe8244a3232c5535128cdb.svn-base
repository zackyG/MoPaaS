<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/svclistview_item_height"
    android:descendantFocusability="blocksDescendants"
     >
    <ImageView
        android:id="@+id/labelimg"
        android:layout_width="@dimen/appicon_width"
        android:layout_height="@dimen/appicon_height"
        android:layout_marginLeft="10dip"
        android:layout_marginTop="10dip"
        android:layout_marginBottom="10dip"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        />
    <TextView
        android:id="@+id/deletebtn"
        android:layout_width="@dimen/appicon_width"
        android:layout_height="@dimen/svclistview_item_height"
        android:layout_alignParentRight="true"
        android:background="@color/red"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:text="@string/delete"
        android:gravity="center"
        android:visibility="gone"
        />
	<RelativeLayout
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:layout_toLeftOf="@+id/deletebtn"
	    android:layout_toRightOf="@+id/labelimg"
	    android:layout_marginLeft="10dip"
	    android:layout_marginRight="10dip"
	    android:layout_centerVertical="true"
	    >
	    <TextView
	        android:id="@+id/svcname"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentTop="true"
	        android:layout_alignParentLeft="true"
	        android:gravity="left"
	        android:textColor="#000000"
	        android:textSize="20sp"
	        />
	    <TextView
	        android:id="@+id/createtime"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/svcname"
	        android:layout_alignParentRight="true"
	        android:gravity="right"
	        android:textColor="#929292"
	        android:textSize="15sp"
	        />
	    <TextView
	        android:id="@+id/appbind"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_alignLeft="@+id/svcname"
	        android:layout_below="@+id/svcname"
	        android:gravity="left"
	        android:textColor="#929292"
	        android:textSize="15sp"
	        />
	</RelativeLayout>
</RelativeLayout>
